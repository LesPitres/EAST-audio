<xs:simpleType name="RGBcolor">
    <xs:restriction base="xs:string">
        <xs:pattern value='#[0-9A-F]{6}'/>
    </xs:restriction>
</xs:simpleType>

<xs:simpleType name="Allcolors">
    <xs:union memberTypes="CSScolor W3Ccolor RGBcolor"/>
</xs:simpleType>

<xs:simpleType name="list_mode">
<xs:restriction base="xs:string">
<xs:enumeration value="accordeon"/>
<xs:enumeration value="pliage"/>
<xs:enumeration value="incremental"/>
<xs:enumeration value="incremental_ombre"/>
<xs:enumeration value="incremental_allume"/>
<xs:enumeration value="progressif"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="transitiontype">
<xs:restriction base="xs:string">
<xs:enumeration value="none"/>
<xs:enumeration value="crossfade"/>
<xs:enumeration value="fadethrough"/>
<xs:enumeration value="carousel"/>
<xs:enumeration value="slide-in"/>
<xs:enumeration value="toss"/>
<xs:enumeration value="burst"/>
</xs:restriction>
</xs:simpleType>

<xs:element name="EL">
<xs:annotation>
<xs:documentation>
Elément de liste
</xs:documentation>
</xs:annotation>
<xs:complexType mixed="true">
<xs:choice maxOccurs="unbounded" minOccurs="0">
<xs:element ref="LISTE"/>
<xs:group ref="texte"/>
<xs:element ref="COMMENTAIRE"/>
</xs:choice>

</xs:complexType>
</xs:element>

<xs:element name="LISTEDEF">
<xs:annotation>
<xs:documentation>
Liste de définitions
</xs:documentation>
</xs:annotation>
<xs:complexType>
<xs:sequence>
<xs:element maxOccurs="1" minOccurs="0" ref="TITRE"/>
<xs:sequence maxOccurs="unbounded" minOccurs="1">
<xs:element ref="TERME"/>
<xs:element ref="DEF"/>
</xs:sequence>
</xs:sequence>
</xs:complexType>
</xs:element>

<xs:element name="TERME">
<xs:annotation>
<xs:documentation>
Terme d'une liste de définitions
</xs:documentation>
</xs:annotation>
<xs:complexType mixed="true">
<xs:group maxOccurs="unbounded" minOccurs="0" ref="texte"/>
</xs:complexType>
</xs:element>

<xs:element name="DEF">
<xs:annotation>
<xs:documentation>
Définition pour une liste de definitions
</xs:documentation>
</xs:annotation>
<xs:complexType mixed="true">
<xs:group maxOccurs="unbounded" minOccurs="0" ref="texte"/>
</xs:complexType>
</xs:element>


<xs:element name="TABLEAU">
<xs:complexType>
<xs:sequence maxOccurs="unbounded">
<xs:element ref="LT"/>
</xs:sequence>
</xs:complexType>
</xs:element>

<xs:element name="LT">
<xs:annotation>
<xs:documentation>
Ligne de tableau
</xs:documentation>
</xs:annotation>
<xs:complexType>
<xs:choice>
<xs:sequence>
<xs:element maxOccurs="unbounded" minOccurs="0" ref="ET"/>
</xs:sequence>
<xs:sequence>
<xs:element maxOccurs="unbounded" minOccurs="0" ref="CT"/>
</xs:sequence>
</xs:choice>
</xs:complexType>
</xs:element>

<xs:element name="CT">
<xs:annotation>
<xs:documentation>
Cellule de tableau
</xs:documentation>
</xs:annotation>
<xs:complexType mixed="true">
<xs:choice>    
<xs:group maxOccurs="unbounded" minOccurs="0" ref="texte"/>
<xs:element maxOccurs="unbounded" minOccurs="0" ref="LISTE"/>
</xs:choice>  
<xs:attribute name="rowspan" type="xs:positiveInteger" use="optional"/>
<xs:attribute name="colspan" type="xs:positiveInteger" use="optional"/>
       <xs:attribute name="textcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="backcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="image" type="xs:anyURI" use="optional"/>
        <xs:attribute name="font" type="xs:string" use="optional"/>
        
</xs:complexType>
</xs:element>

<xs:element name="ET">
<xs:annotation>
<xs:documentation>
Cellule entête de tableau
</xs:documentation>
</xs:annotation>
<xs:complexType mixed="true">
<xs:group maxOccurs="unbounded" minOccurs="0" ref="texte"/>
<xs:attribute name="colspan" type="xs:positiveInteger" use="optional"/>
       <xs:attribute name="textcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="backcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="image" type="xs:anyURI" use="optional"/>
        <xs:attribute name="font" type="xs:string" use="optional"/>
        
</xs:complexType>
</xs:element>

<xs:element name="EXEMPLE">
<xs:complexType mixed="true">
<xs:sequence>
<xs:element maxOccurs="1" minOccurs="0" ref="TITRE"/>
<xs:group maxOccurs="unbounded" minOccurs="0" ref="texte"/>
</xs:sequence>
<xs:attribute name="label" type="xs:string" use="optional"/>
</xs:complexType>
</xs:element>

<xs:group name="texte">
    
<xs:annotation>
    
    <xs:documentation>
        
        Ensemble de texte et d'éléments mélanges à  du texte
    
    </xs:documentation>
    
</xs:annotation>

<xs:choice>
    
    <xs:element ref="EMPHASE"/>
    <xs:element ref="CODE"/>
    <xs:element ref="CODE_COLS"/>
    <xs:element ref="ESP"/>
    <xs:element ref="BR"/>
    <xs:element ref="LIEN_EXTERNE"/>
    <xs:element ref="LIEN_INTERNE"/>
    <xs:element ref="IMAGE"/>
    <xs:element ref="VIDEO"/>
    <xs:element ref="EQUATION"/>
    <xs:element ref="EXEMPLE"/>

</xs:choice>

</xs:group>

<xs:element name="EMPHASE" type="xs:string">
</xs:element>

<xs:element name="CODE">

    <xs:complexType>
    
        <xs:simpleContent>
            
            <xs:extension base="xs:string">
                
                <xs:attribute name="textcolor" type="Allcolors" use="optional"/>
                <xs:attribute name="backcolor" type="Allcolors" use="optional"/>
                <xs:attribute name="image" type="xs:anyURI" use="optional"/>
                <xs:attribute name="font" type="xs:string" use="optional"/>
                
            </xs:extension>
            
        </xs:simpleContent>

    </xs:complexType>
    
</xs:element>

<xs:element name="CODE_COLS">
    
    <xs:complexType mixed="true">
        
        <xs:group maxOccurs="unbounded" minOccurs="0" ref="texte"/>
        
        <xs:attribute name="nbcols" type="xs:integer" use="optional"/>
        <xs:attribute name="textcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="backcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="image" type="xs:anyURI" use="optional"/>
        <xs:attribute name="font" type="xs:string" use="optional"/>
        
    </xs:complexType>
    
</xs:element>


<xs:element name="ESP">
<xs:annotation>
<xs:documentation>
Espace insécable.
</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="BR">
<xs:annotation>
<xs:documentation>
Saut de ligne. Utiliser PARAGRAPHE de préférence pour séparer les paragraphes.
</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="LIEN_EXTERNE">
<xs:complexType mixed="true">
<xs:attribute name="href" type="xs:anyURI" use="required"/>
</xs:complexType>
</xs:element>

<xs:element name="LIEN_INTERNE">
<xs:complexType mixed="true">
<xs:attribute name="num" type="xs:integer" use="required"/>
</xs:complexType>
</xs:element>

<xs:element name="EQUATION">
<xs:complexType>
<xs:attribute name="texte" type="xs:string" use="required"/>
<xs:attribute name="image" type="xs:anyURI" use="required"/>
</xs:complexType>
</xs:element>

<xs:element name="IMAGE">
<xs:complexType>
<xs:attribute name="fichier" type="xs:anyURI" use="required"/>
<xs:attribute name="alt" type="xs:string" use="optional"/>
<xs:attribute name="largeur" type="percentage" use="optional"/>
<xs:attribute name="hauteur" type="percentage" use="optional"/>
</xs:complexType>
</xs:element>

<xs:element name="ENVIMAGE">
<xs:annotation>
<xs:documentation>
Environnement pour une image, avec un titre et une légende.
</xs:documentation>
</xs:annotation>
<xs:complexType>
<xs:sequence>
<xs:element maxOccurs="1" minOccurs="0" ref="TITRE"/>
<xs:element maxOccurs="1" minOccurs="1" ref="IMAGE"/>
<xs:element maxOccurs="1" minOccurs="0" ref="LEGENDE"/>
</xs:sequence>
        <xs:attribute name="titlecolor" type="Allcolors" use="optional"/>
        <xs:attribute name="titlefont" type="xs:string" use="optional"/>
        <xs:attribute name="titleback" type="Allcolors" use="optional"/>
        <xs:attribute name="textcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="backcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="image" type="xs:anyURI" use="optional"/>
        <xs:attribute name="font" type="xs:string" use="optional"/>
</xs:complexType>
</xs:element>

<xs:element name="VIDEO">
<xs:complexType>
<xs:attribute name="fichier" type="xs:anyURI" use="required"/>
<xs:attribute name="largeur" type="percentage" use="optional"/>
<xs:attribute name="hauteur" type="percentage" use="optional"/>
<xs:attribute name="controls" type="xs:boolean" use="optional"/>
<xs:attribute name="poster" type="xs:anyURI" use="optional"/>
<xs:attribute name="autoplay" type="xs:boolean" use="optional"/>
</xs:complexType>
</xs:element>



<xs:element name="ENVIDEO">
<xs:annotation>
<xs:documentation>
Environnement pour une video, avec un titre et une légende.
</xs:documentation>
</xs:annotation>
<xs:complexType>
<xs:sequence>
<xs:element maxOccurs="1" minOccurs="0" ref="TITRE"/>
<xs:element maxOccurs="1" minOccurs="1" ref="VIDEO"/>
<xs:element maxOccurs="1" minOccurs="0" ref="LEGENDE"/>
</xs:sequence>
        <xs:attribute name="titlecolor" type="Allcolors" use="optional"/>
        <xs:attribute name="titlefont" type="xs:string" use="optional"/>
        <xs:attribute name="titleback" type="Allcolors" use="optional"/>
        <xs:attribute name="image" type="xs:anyURI" use="optional"/>
        <xs:attribute name="textcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="backcolor" type="Allcolors" use="optional"/>
        <xs:attribute name="font" type="xs:string" use="optional"/>
</xs:complexType>
</xs:element>

<xs:element name="LEGENDE">
<xs:complexType mixed="true">
<xs:group maxOccurs="unbounded" minOccurs="0" ref="texte"/>
</xs:complexType>
</xs:element>

<xs:element name="COMMENTAIRE">
<xs:annotation>
<xs:documentation>
Texte qui ne sera pas affiché en sortie, sauf si le paramêtre "commentaires" de la feuille de style a la valeur "oui".
</xs:documentation>
</xs:annotation>
<xs:complexType mixed="true">
<xs:choice maxOccurs="unbounded" minOccurs="0">
<xs:element ref="PARAGRAPHE"/>
<xs:element ref="LISTE"/>
<xs:element ref="LISTEDEF"/>
<xs:element ref="TABLEAU"/>
<xs:group ref="texte"/>
</xs:choice>
</xs:complexType>
</xs:element>

<xs:element name="HTML">
<xs:annotation>
<xs:documentation>
Code HTML Ã  afficher "tel quel"
</xs:documentation>
</xs:annotation>
<xs:complexType mixed="true">
</xs:complexType>
</xs:element>

<xs:element name="APPLET">
<xs:annotation>
<xs:documentation>
Appliquette Java.
</xs:documentation>
</xs:annotation>
<xs:complexType>
<xs:choice maxOccurs="unbounded" minOccurs="0">
<xs:element ref="PARAM"/>
</xs:choice>
<xs:attribute name="classe" type="xs:string" use="required"/>
<xs:attribute name="archive" type="xs:string" use="optional"/>
<xs:attribute name="largeur" type="xs:positiveInteger" use="required"/>
<xs:attribute name="hauteur" type="xs:positiveInteger" use="required"/>
</xs:complexType>
</xs:element>

<xs:element name="PARAM">
<xs:annotation>
<xs:documentation>
parametre d'applet
</xs:documentation>
</xs:annotation>
<xs:complexType>
<xs:attribute name="nom" type="xs:string" use="required"/>
<xs:attribute name="valeur" type="xs:string" use="required"/>
</xs:complexType>
</xs:element>
</xs:schema>
